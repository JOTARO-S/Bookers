<h1>Books</h1>
<div class="books-content">
  <p>Title</p>
  <p>Body</p>
</div>
  
<div class ="books-wrap">  
  <% @books.each do |books| %>
    <div class="books-content">
      <span><%= books.title %></span>
      <span><%= books.body %></span>
      
    <% end %>
</div>



<h2>New book</h2>
<%= form_with model: @book do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  
  <h4>Body</h4>
  <%= f.text_area :body %>
  
  <%= f.submit 'Create Book' %>
<% end %>